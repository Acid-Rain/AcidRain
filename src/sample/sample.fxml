<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane fx:id="gamePane" prefHeight="768.0" prefWidth="1024.0" GridPane.rowIndex="1">
         <children>
            <ImageView fitHeight="768.0" fitWidth="1024.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@img/game_bg.png" />
               </image>
            </ImageView>
            <TextField fx:id="textInput" layoutX="429.0" layoutY="707.0" />
            <Button fx:id="startButton1" layoutX="603.0" layoutY="706.0" mnemonicParsing="false" text="Game Start" />
            <ProgressBar fx:id="lifeBar" layoutX="111.0" layoutY="712.0" prefWidth="200.0" progress="100.0" />
         </children></Pane>
   </children>
</GridPane>
