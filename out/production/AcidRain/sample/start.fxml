            <ImageView fitHeight="768.0" fitWidth="1024.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@img/menu_bg.png" />
               </image>
            </ImageView>
            <Button fx:id="startButton" layoutX="99.0" layoutY="311.0" mnemonicParsing="false" prefHeight="85.0" prefWidth="211.0" style="-fx-opacity: 0;" text="Button" />
            <Button fx:id="helpButton" layoutX="423.0" layoutY="316.0" mnemonicParsing="false" prefHeight="66.0" prefWidth="179.0" style="-fx-opacity: 0;" text="Button" />
            <Button fx:id="exitButton" layoutX="741.0" layoutY="315.0" mnemonicParsing="false" prefHeight="77.0" prefWidth="163.0" style="-fx-opacity: 0;" text="Button" />
